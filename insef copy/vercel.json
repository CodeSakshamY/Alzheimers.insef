{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/predict", "dest": "Alzheimers.insef/insef.copy/api/predict.py" },
    { "src": "/", "dest": "Alzheimers.insef/insef.copy/public/index.html" }
  ]
}
